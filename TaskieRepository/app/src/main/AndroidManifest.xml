<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="hr.ferit.zvonimirpavlovic.taskierepository">

    <application
            android:name="hr.ferit.zvonimirpavlovic.taskierepository.Taskie"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="hr.ferit.zvonimirpavlovic.taskierepository.ui.activities.MainActivity"
                  android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name="hr.ferit.zvonimirpavlovic.taskierepository.ui.activities.ContainerActivity"
                android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>